{
  "name": "cloud-management-dashboard",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "deploy:frontend": "npm run build && npx wrangler pages deploy dist",
    "deploy:backend": "npx wrangler deploy --config wrangler.worker.toml --env production",
    "dev:backend": "wrangler dev --config wrangler.worker.toml --env development"
  },
  "dependencies": {
    "@headlessui/react": "^1.7.18",
    "axios": "^1.6.7",
    "bcryptjs": "^2.4.3",
    "lucide-react": "^0.344.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.22.3",
    "recharts": "^2.12.2"
  },
  "devDependencies": {
    "@types/react": "^18.2.64",
    "@types/react-dom": "^18.2.21",
    "@typescript-eslint/eslint-plugin": "^7.1.1",
    "@typescript-eslint/parser": "^7.1.1",
    "@vitejs/plugin-react": "^4.3.3",
    "autoprefixer": "^10.4.18",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.2.2",
    "vite": "^7.0.5",
    "wrangler": "^4.25.0"
  }
}
